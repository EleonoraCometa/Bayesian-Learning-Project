data = read.csv("indprod.csv")
data$DATE = as.Date(data$DATE)
ggplot(data, aes(x = DATE, y = INDPRO_PCH)) +
# Linea principale dell'IPI
geom_line(color = "black", size = 1.2) +
# Aggiunta di una media mobile semplice a 3 giorni
geom_smooth(method = "loess", se = FALSE, color = "blue", size = 0.8, linetype = "dashed") +
# Titoli e etichette
labs(title = "Time-Series of IPI",
subtitle = "Industrial Production Index (IPI) over Time",
x = "Date", y = "IPI Index",
caption = "Source: Example Data") +
# Miglioramento del tema
theme_minimal(base_size = 15) +
theme(
panel.grid.major = element_line(color = "lightgray", size = 0.5),
panel.grid.minor = element_line(color = "lightgray", size = 0.25),
axis.line = element_line(color = "black"),
axis.ticks = element_line(color = "black"),
plot.title = element_text(face = "bold", size = 20, hjust = 0.5),
plot.subtitle = element_text(size = 14, hjust = 0.5),
plot.caption = element_text(size = 10, hjust = 1, color = "gray"),
axis.title = element_text(face = "bold"),
axis.text = element_text(size = 12)
) +
# Annotazione di un punto importante (esempio)
annotate("text", x = as.Date("2023-01-04"), y = 105, label = "High Point",
vjust = -1, color = "red", fontface = "italic", size = 4) +
# Limiti degli assi
scale_x_date(date_labels = "%b %Y", date_breaks = "1 month") +
scale_y_continuous(labels = scales::comma)
